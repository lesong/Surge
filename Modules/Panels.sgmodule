#!name=Panels
#!desc=信息面板

[Panel]
#Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=1

#流量统计
TrafficStatistics = script-name=TrafficStatistics,update-interval=1

#网络详情
网络信息 = script-name=网络信息,update-interval=1

#机场信息
Sub_info = script-name=Sub_info,update-interval=600

#配置重载
配置重载 = title=配置重载,content=配置重载,style=info,script-name=配置重载,update-interval=-1

#策略组面板 可重复配置 注意修改相应字段
🚀 节点选择 = script-name=🚀 节点选择,update-interval=5
🔰 Outside = script-name=🔰 Outside,update-interval=5
🎬 Streaming = script-name=🎬 Streaming,update-interval=5
📺 CNTV = script-name=📺 CNTV,update-interval=5
🍎 Apple = script-name=🍎 Apple,update-interval=5
📬 Telegram = script-name=📬 Telegram,update-interval=5
💳 PayPal = script-name=💳 PayPal,update-interval=5
🎵 Tiktok = script-name=🎵 Tiktok,update-interval=5
🎯 Inbound = script-name=🎯 Inbound,update-interval=5
🚧 AdBlock = script-name=🚧 AdBlock,update-interval=5
🐟 Outbound = script-name=🐟 Outbound,update-interval=5
🇭🇰 Hong Kong = script-name=🇭🇰 Hong Kong,update-interval=5
🇨🇳 Taiwan = script-name=🇨🇳 Taiwan,update-interval=5
🇯🇵 Japan = script-name=🇯🇵 Japan,update-interval=5
🇸🇬 Singapore = script-name=🇸🇬 Singapore,update-interval=5
🇺🇸 America = script-name=🇺🇸 America,update-interval=5
🇰🇷 Korea = script-name=🇰🇷 Korea,update-interval=5
🇬🇧 England = script-name=🇬🇧 England,update-interval=5
🇧🇷 Brazil = script-name=🇧🇷 Brazil,update-interval=5
🇩🇪 Germany = script-name=🇩🇪 Germany,update-interval=5
🇮🇳 India = script-name=🇮🇳 India,update-interval=5
🇲🇾 Malaysia = script-name=🇲🇾 Malaysia,update-interval=5
🇷🇺 Russia = script-name=🇷🇺 Russia,update-interval=5
🇹🇷 Turkey = script-name=🇹🇷 Turkey,update-interval=5
🇦🇷 Argentina = script-name=🇦🇷 Argentina,update-interval=5

[Script]
#附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js ,argument=icon=crown.fill&color=#0089A7

#流量统计 点击以切换网络界面
TrafficStatistics = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js ,argument=icon=arrow.up.arrow.down.circle&color=#0089A7

#網路詳情 标题显示为根节点名
#应当修改的字段 group 代理策略组名称
网络信息 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/TributePaulWalker/Profiles/main/JavaScript/Surge/ipcheck.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=Master

#机场信息
#必须修改的字段:你encode后的机场订阅链接
Sub_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js,script-update-interval=0,argument=url=https%3A%2F%2Fcylink.moe%2Flink%2FhHAPqElxSjyguFWw%3Flist%3Dsurge&reset_day=1&title=机场信息&icon=airplane.circle&color=#0089A7

配置重载=script-path=https://raw.githubusercontent.com/TributePaulWalker/Profiles/main/JavaScript/Surge/Profile-Reload.js,type=generic

#策略组面板 可重复配置 注意修改相应字段
